{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext", "ES2015"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/sectionComponents/threeScene.jsx", "src/components/GSAPparallax.jsx", "src/app/(user)/page.jsx", "src/pages/services.tsx", "src/components/sectionComponents/menu.jsx", "src/components/parallax/scrollparallaxpage.jsx", "src/components/csrDataFetch.jsx", "src/components/accordionForm.jsx", "src/components/styledComponents/button.jsx", "src/pages/home.tsx", "src/app/(user)/layout.tsx"],
  "exclude": ["node_modules"]
}
 